{
    "name": "hasob/vanilla-lms",
    "type": "project",
    "description": "Vanilla Learning Management System.",
    "keywords": ["vanilla", "lms"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "appointer/swaggervel": "dev-master",
        "barryvdh/laravel-snappy": "^0.4.8",
        "doctrine/dbal": "^3.1",
        "dompdf/dompdf": "^1.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "infyomlabs/adminlte-templates": "^2.0",
        "infyomlabs/laravel-generator": "^8.0.0",
        "infyomlabs/swagger-generator": "dev-master",
        "joisarjignesh/bigbluebutton": "^1.1",
        "knplabs/knp-snappy": "^1.2",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "laravel/ui": "3.0",
        "laravelcollective/html": "^6.2",
        "sentry/sentry-laravel": "^2.8",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.10",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },

    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:InfyOmLabs/swaggervel.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
